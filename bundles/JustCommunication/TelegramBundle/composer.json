{
    "name": "justcommunication/telegram-bundle",
    "description": "Telegram bundle",
    "version": "0.0.1",
    "type": "symfony-bundle",
    "keywords": ["telegram","messages","notification"],
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.15",
        "symfony/framework-bundle": "^6.3",
        "symfony/yaml": "^6.3"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JustCommunication\\TelegramBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustCommunication\\TelegramBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/browser-kit": "^6.3",
        "symfony/phpunit-bridge": "^6.3"
    }
}
